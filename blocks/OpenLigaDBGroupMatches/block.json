{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "soccr/group-matches",
  "title": "Soccr Spieltag",
  "category": "soccr",
  "icon": "forms",
  "description": "OpenligaDB Spieltag",
  "keywords": [ "OpenligaDB", "liga", "spieltag", "soccr"],
  "textdomain": "soccr",
  "attributes": {
    "leagueShortcut": {
      "type": "string",
      "default": "bl2"
    },
    "leagueSeason": {
      "type": "number",
      "default": 2021
    },
    "defaultCurrentGroup": {
      "type": "boolean",
      "default": true
    },
    "groupOrderId": {
      "type": "number",
      "default": 1
    },
    "pagination": {
      "type": "boolean",
      "default": true
    },
    "align": {
      "type": "string",
      "default": "center"
    },
    "blockId": {
      "type": "string"
    }
  },
  "supports": {
    "align": true
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./style.css",
  "style": "file:./style.scss"
}
